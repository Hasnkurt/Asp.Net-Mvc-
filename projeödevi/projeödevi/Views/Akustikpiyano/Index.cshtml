    <style>
        body {
            background-color: white;
            font-family: 'Oxygen', sans-serif;
            font-size: 14px;
        }


        .card .backmenu ul {
            display: flex;
        }

            .card .backmenu ul li {
                list-style: none;
                width: 15px;
                height: 15px;
                margin: 5px;
                cursor: pointer;
            }

                .card .backmenu ul li:nth-child(1) {
                    background: brown;
                }


                .card .backmenu ul li:nth-child(2) {
                    background: black;
                }




                .card .backmenu ul li:nth-child(3) {
                    background: white;
                }



        .card {
            width: 300px;
            height: 400px;
            background: #f4f4f4;
            position: relative;
            border-radius: 12px;
            box-shadow: 0;
            transform: scale(0.95);
            transition: .5s ease-in-out;
            transition box-shadow:.5s ease-in-out;
            transform: .5s ease-in-out;
            overflow: hidden;
            margin-top: 50px;
        }





            .card:hover {
                transform: scale(1);
                box-shadow: 5px 20px 30px rgba(0,0,0,.2);
            }

        .imgBox {
        }

        img {
            width: 100%;
            height: 100%;
        }

        .details {
            position: absolute;
            width: 100%;
            height: -50%;
            background: #f4f4f4be;
            padding: 10px 20px;
            bottom: -27%;
            transition: .5s;
        }

            .details:hover {
                bottom: 0;
            }

        .textcontent {
            position: relative;
            margin-top: 10px;
            color: #555;
        }

        .fiyat {
            position: absolute;
            top: 0;
            right: 0;
            font-size: 23px;
            text-decoration: line-through;
        }

        .indirim {
            top: 0px;
            padding: 2px;
            margin-right: 20%;
            font-size: 23px;
        }




        button:hover {
            background: #5acef4;
        }

        .description {
            background: #92879b;
            position: absolute;
            width: 140px;
            height: 140px;
            top: -70px;
            right: -70px;
            border-radius: 0 0 200px 200px;
            transition: all .5s, border-radius 1s,top 1s;
        }

            .description.icon {
                position: absolute;
                top: 85px;
                right: 85px;
                color: #fff;
                opacity: 1;
            }

            .description:hover {
                width: 100%;
                height: 80%;
                right: 0;
                top: 0;
                border-radius: 0;
            }

                .description:hover.icon {
                    opacity: 0;
                }

            .description .contents {
                color: #fff;
                padding: 18%;
                transform: scale(0.5);
                transform: translateY(-200%);
                transition: opacity .2s,transform .8s;
                opacity: 0;
            }

            .description:hover .contents {
                transform: scale(1);
                opacity: 1;
                transform: translateY(0%);
            }

            .description h2 {
                margin: 5px;
                text-align: center;
            }

        .row {
            display: flex;
            flex-wrap: wrap;
        }

        .footer {
            background-color: black;
            padding: 20px 0;
        }


        .footer-col ul {
            list-style: none;
            width: 100%;
        }

        .footer-col {
            width: 25%;
            padding: 0 15px;
        }

        .footer .container {
            margin: auto;
        }

        .footer-col h4 {
            font-size: 18px;
            color: white;
            text-transform: capitalize;
            margin-bottom: 20px;
            font-weight: 500;
            position: relative;
            text-align: center;
        }

            .footer-col h4::before {
                content: '';
                position: absolute;
                left: auto;
                bottom: -10px;
                background-color: #E91e63;
                height: 2px;
                box-sizing: border-box;
                width: 65%;
            }

        .footer-col ul li:not(:last-child) {
            margin-bottom: 9px;
        }

        .footer-col ul li a {
            font-size: 17px;
            text-transform: capitalize;
            color: white;
            text-decoration: none;
            font-weight: 400;
            color: #bbbbbb;
            display: block;
            transition: all 0.6s ease;
            text-align: center;
        }

            .footer-col ul li a:hover {
                color: #ffffff;
                padding-left: 8px;
            }

        .footer-col .social-links a {
            display: inline-block;
            height: 40px;
            width: 40px;
            background-color: rgba(255,255,255,0.2);
            margin: 0 10px 10px 0;
            text-align: center;
            line-height: 40px;
            border-radius: 50%;
            color: white;
            transition: all 0.5s ease;
        }

            .footer-col .social-links a:hover {
                color: #24262b;
                background-color: white;
                text-align: center;
            }
        .social-links {
            text-align: center;
        }

        .sekmeler li a {
            color: black;
        }
